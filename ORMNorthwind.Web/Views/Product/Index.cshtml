@model IEnumerable<ORMNorthwind.Entities.Concrete.Products>
@{
    ViewBag.Title = "Index";
}


<table class="table table-striped">
    <tr>
        <th>ID</th>
        <th>Product Name</th>
        <th>Units available</th>
        <th>Discontinued</th>
    </tr>
    @foreach(var p in Model)
    {

        <tr>
            <td>@p.ProductID</td>
            <td>@p.ProductName</td>
            <td>@p.UnitsInStock</td>
            <td>@p.Discontinued</td>
        </tr>
    }
</table>

